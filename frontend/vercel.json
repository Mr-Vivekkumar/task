{
  "version": 2,
  "builds": [
    {
      "src": "dist/frontend/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/frontend/browser/$1"
    },
    {
      "src": "/",
      "dest": "/dist/frontend/browser/index.html"
    }
  ],
  "outputDirectory": "dist/frontend/browser",
  "installCommand": "npm install",
  "buildCommand": "npm run build:prod",
  "framework": "angular"
}
